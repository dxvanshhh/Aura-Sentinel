{
  "manifest_version": 3,
  "name": "AuraSentinel",
  "version": "1.0",
  "description": "An AI-powered phishing detector.",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon48.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": ["http://127.0.0.1:5000/*"]
}